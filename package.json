{
  "name": "packcdn",
  "version": "1.0.0",
  "type": "module",
  "description": "Custom package CDN system for publishing and sharing npm, Python, and WebAssembly packages with automatic CDN generation",
  "main": "selector.html",
  "scripts": {
    "dev": "vercel dev",
    "start": "vercel dev",
    "deploy": "vercel --prod",
    "build": "echo 'No build step required for static site'",
    "test": "echo \"No tests specified\" && exit 0",
    "setup": "node scripts/setup-db.js"
  },
  "keywords": [
    "cdn",
    "package-manager",
    "npm",
    "python",
    "wasm",
    "cloudflare",
    "vercel",
    "supabase",
    "monaco-editor",
    "secure-login",
    "anonymous-auth"
  ],
  "author": "PackCDN Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "nodemailer": "^6.9.7",
    "uuid": "^9.0.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cookie": "^0.5.0",
    "crypto": "^1.0.1",
    "@vercel/node": "^2.15.3",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "@wasmer/wasi": "^1.2.2",
    "@wasmer/wasm-transformer": "^0.12.0",
    "@wasmer/sdk": "^0.10.0"
  },
  "devDependencies": {
    "vercel": "^34.0.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/packcdn.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/packcdn/issues"
  },
  "homepage": "https://pack-cdn.vercel.app",
  "files": [
    "*.html",
    "*.js",
    "api/*.js",
    "public/*",
    "scripts/*.js"
  ]
}
