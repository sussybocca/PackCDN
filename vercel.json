{
  "rewrites": [
    {
      "source": "/",
      "destination": "/selector.html"
    },
    {
      "source": "/home",
      "destination": "/home/index.html"
    },
    {
      "source": "/docs",
      "destination": "/docs.html"
    },
    {
      "source": "/editor",
      "destination": "/editor.html"
    },
    {
      "source": "/explore",
      "destination": "/explore.html"
    },
    {
      "source": "/config.json",
      "destination": "/config.json"
    },
    {
      "source": "/api/random-urls",
      "destination": "/api/random-urls"
    },
    {
      "source": "/api/wildcard-redirect",
      "destination": "/api/wildcard-redirect"
    },
    {
      "source": "/((?!.*\\..*).*)",
      "destination": "/api/wildcard-redirect"
    }
  ],
  "redirects": [
    {
      "source": "/portal",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/gateway",
      "destination": "/home",
      "permanent": true
    },
    {
      "source": "/library",
      "destination": "/docs",
      "permanent": true
    },
    {
      "source": "/workshop",
      "destination": "/editor",
      "permanent": true
    },
    {
      "source": "/discover",
      "destination": "/explore",
      "permanent": true
    },
    {
      "source": "/settings",
      "destination": "/config.json",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/config.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false
}
