{
  "rewrites": [
    {
      "source": "/api/random-urls",
      "destination": "/api/random-urls"
    },
    {
      "source": "/api/wildcard-redirect",
      "destination": "/api/wildcard-redirect"
    },
    {
      "source": "/(.*)",
      "destination": "/api/wildcard-redirect"
    }
  ],
  "redirects": [
    {
      "source": "/portal",
      "destination": "/",
      "permanent": false
    },
    {
      "source": "/gateway",
      "destination": "/home",
      "permanent": false
    },
    {
      "source": "/library",
      "destination": "/docs",
      "permanent": false
    },
    {
      "source": "/workshop",
      "destination": "/editor",
      "permanent": false
    },
    {
      "source": "/discover",
      "destination": "/explore",
      "permanent": false
    },
    {
      "source": "/settings",
      "destination": "/config.json",
      "permanent": false
    }
  ],
  "headers": [
    {
      "source": "/config.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ],
  "cleanUrls": false,
  "trailingSlash": false
}
