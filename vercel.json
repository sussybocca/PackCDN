// vercel.json
{
  "rewrites": [
    {
      "source": "/",
      "destination": "/selector.html"
    },
    {
      "source": "/home",
      "destination": "/home/index.html"
    },
    {
      "source": "/docs",
      "destination": "/docs.html"
    },
    {
      "source": "/editor",
      "destination": "/editor.html"
    },
    {
      "source": "/explore",
      "destination": "/explore.html"
    }
  ],
  "cleanUrls": false,  // Set to false since we're using .html explicitly
  "trailingSlash": false
}
